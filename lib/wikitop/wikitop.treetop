grammar Wikitop
  rule document
    phrase
  end

  rule phrase
    word_or_link* <Phrase>
  end

  rule word_or_link
    wiki_link
    /
    word
  end

  rule wiki_link
    wiki_link_start word wiki_link_end <WikiLink>
  end

  rule word
    char+ <String>
  end

  rule char
    [a-zA-Z0-9 \n\t*!@#$%^&()_{}\|/?.>,<`~]
  end

  rule left_bracket
    '['
  end

  rule wiki_link_start
    '[['
  end

  rule wiki_link_end
    ']]'
  end
end
